<%= form_for @user do |f| %>
  <%= f.label :name, @user.name %>
  <%= f.text_field :name %>

  <%= f.label :username, @user.username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.password_field :password %>

    <%= f.label :group %>
    <%= f.text_field :group %>

  <%= f.submit %>
<% end %>
